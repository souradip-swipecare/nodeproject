                                                                    <thead>
                                                                        <tr class="bg-primary text-white">
                                                                            <th class="sorting">Opimg</th>
                                                                            <th class="sorting">ID</th>
                                                                            <th class="sorting">Name</th>
                                                                            <th class="sorting">Opcode</th>
                                                                            <th class="sorting">Commision type</th>
                                                                            <th class="sorting">Amount</th>

                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        
                                                                        <tr class="even">
                                                                            
                                                                            <td style="margin-right: 10px;"><div class="col-md-6"><img src="/adminup/operator/" width="60px" height="60px"></td></div>
                                                                            <td style="margin-right: 30px;"></td>
                                                                            <td></td>
                                                                            <td class="sorting">
                                                                                <div class="col-md-4">
                                                                                    <div class="form-group row">
                                                                                        <div>
                                                                                            <input class="form-control" type="text" name="opvalue" required="true" placeholder="ENTER OP Code"
                                                                                                max="5" />
                                                                                        </div>
                                                                                        <label></label>
                                                                        
                                                                                    </div>
                                                                                </div>
                                                                            </td>
                                                                            <td>
                                                                                <div class="col-md-6">
                                                                                    <div class="form-group row">
                                                                                        <select class="form-control" required="true" name="product">
                                                                                            <option value="1">Percentage
                                                                                            </option>
                                                                                            <option value="2">Flat
                                                                                            </option>
                                                                        
                                                                                        </select>
                                                                        
                                                                                    </div>
                                                                                </div>
                                                                                
                                                                            </td>
                                                                            <td>
                                                                                <div class="col-md-6">
                                                                                    <div class="form-group row">
                                                                                        <div>
                                                                                            <input class="form-control" type="number" name="opvalue" required="true" placeholder="ENTER Amount"
                                                                                                max="5" />
                                                                                        </div>
                                                                                        <label></label>
                                                                        
                                                                                    </div>
                                                                                </div>
                                                                            </td>
                                                                        
                                                                      
                                                                        </tr>
                                                                      
                                                                        
                                                                    </tbody>
                                                                </table>
                                                                <div class="col-md-3" style="margin-left: 10px;">
                                                                    <div class="form-group row">

                                                                        <button class="btn btn-gradient-primary"
                                                                            type="submit">
                                                                            <label>Save</label>
                                                                        </button>

                                                                    </div>
                                                                </div>
                                                            </form>
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-sm-12 col-md-5">
                                                            <div class="dataTables_info" id="order-listing_info"
                                                                role="status" aria-live="polite">Showing 1 to 10 of 20
                                                                entries</div>
                                                        </div>
                                                        <div class="col-sm-12 col-md-7">
                                                            <div class="dataTables_paginate paging_simple_numbers"
                                                                id="order-listing_paginate">
                                                                <ul class="pagination">
                                                                    <li class="paginate_button page-item previous disabled"
