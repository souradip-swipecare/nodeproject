<%- include('../../../layouts/user/header') %>

    <%- include('../../../layouts/user/topbar') %>

        <%- include('../../../layouts/user/nabbar') %>

            <!-- recharge section -->
            <div style="margin: 2cm;">
                <div class="col-lg-12 col-md-4 order-1">
                    <div class="row">
                        <div class="col-lg-4 col-md-12 col-6 mb-4">
                            <h4>
                                <span class="text-muted fw-light">Type </span> Request
                            </h4>
                            <div class="card mb-4">
                                <from>
                                    <h5 class="card-header">Type Request</h5>

                                    <div class="card-body">
                                        <div class="mb-3">
                                            <!-- <label for="exampleFormControlInput1" class="form-label">Enter Numer</label> -->
                                            <input type="number" maxlength="10" class="form-control"
                                                id="exampleFormControlInput1" placeholder="Enter Mobile No"  required="true"/>
                                        </div>
                                        <div class="mb-3">

                                            <select class="form-select" id="exampleFormControlSelect1"
                                                aria-label="Default select example"  required="true">
                                                <option selected>Select Payment Mode</option>
                                                <option value="1">One</option>
                                                <option value="2">Two</option>
                                                <option value="3">Three</option>
                                            </select>
                                        </div>
                                        
                                        <div class="mb-3">
                                            <!-- <label for="exampleFormControlInput1" class="form-label">Enter Numer</label> -->
                                            <input type="number" maxlength="4" class="form-control"
                                                id="exampleFormControlInput1" placeholder="Enter Amount" required="true" />
                                        </div>
                                        <div class="mb-3">
                                            <!-- <label for="exampleFormControlInput1" class="form-label">Enter Numer</label> -->
                                            <input type="text" maxlength="12" class="form-control" id="exampleFormControlInput1" placeholder="Enter UTR No" required="true"/>
                                        </div>
                                        <div class="mb-3">
                                            <button type="button" class="btn btn-primary col-md-12">Fund Request</button>
                                        </div>
                                    </div>

                                </from>

                            </div>

                        </div>
                        <!-- recent trasaction -->
                        <div class="col-lg-8 col-md-12 col-6 mb-4">
                            <h4>
                                <span class="text-muted fw-light">Type </span> Recent Request
                            </h4>
                            <div class="card mb-4">

                                <div class="card-datatable table-responsive">
                                    <table class="dt-advanced-search table border-top">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th>Name</th>
                                                <th>Email</th>
                                                <th>Post</th>
                                                <th>City</th>
                                                <th>Date</th>
                                                <th>Salary</th>
                                            </tr>
                                        </thead>

                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- recharge section end -->


            <%- include('../../../layouts/user/footer') %>